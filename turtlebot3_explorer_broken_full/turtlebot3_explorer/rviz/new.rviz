Panels:
  - Class: rviz/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /Status1
        - /Map1
        - /HeuristicHeatMap1
        - /RobotPaths1
        - /HeuristicMarkers1
        - /Frontiers1
      Splitter Ratio: 0.5
    Tree Height: 565
  - Class: rviz/Selection
    Name: Selection
  - Class: rviz/Tool Properties
    Expanded:
      - /2D Pose Estimate1
      - /2D Nav Goal1
      - /Publish Point1
    Name: Tool Properties
    Splitter Ratio: 0.588679
  - Class: rviz/Views
    Expanded:
      - /Current View1
    Name: Views
    Splitter Ratio: 0.5
  - Class: rviz/Time
    Experimental: false
    Name: Time
    SyncMode: 0
    SyncSource: ""
Visualization Manager:
  Class: ""
  Displays:
    - Alpha: 0.5
      Cell Size: 1
      Class: rviz/Grid
      Color: 160; 160; 164
      Enabled: true
      Line Style:
        Line Width: 0.03
        Value: Lines
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 10
      Reference Frame: <Fixed Frame>
      Value: true
    - Alpha: 0.7
      Class: rviz/Map
      Color Scheme: map
      Draw Behind: false
      Enabled: true
      Name: Map
      Topic: /map
      Unreliable: false
      Use Timestamp: false
      Value: true
    - Alpha: 0.4
      Class: rviz/Map
      Color Scheme: costmap
      Draw Behind: false
      Enabled: true
      Name: HeuristicHeatMap
      Topic: /heuristic_heat_map
      Unreliable: false
      Use Timestamp: false
      Value: true
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /robot_path_markers
      Name: RobotPaths
      Namespaces:
        robot_paths: true
      Queue Size: 100
      Value: true
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /heuristic_markers
      Name: HeuristicMarkers
      Namespaces:
        heat_map: true
      Queue Size: 100
      Value: true
    - Alpha: 1
      Arrow Length: 0.3
      Axes Length: 0.3
      Axes Radius: 0.01
      Class: rviz/PoseArray
      Color: 255; 25; 0
      Enabled: true
      Head Length: 0.07
      Head Radius: 0.03
      Name: Frontiers
      Shaft Length: 0.23
      Shaft Radius: 0.01
      Shape: Arrow (Flat)
      Topic: /frontiers
      Unreliable: false
      Value: true
    - Class: rviz/Group
      Displays:
        - Alpha: 1
          Class: rviz/RobotModel
          Collision Enabled: false
          Enabled: true
          Links:
            All Links Enabled: true
            Expand Joint Details: false
            Expand Link Details: false
            Expand Tree: false
            Link Tree Style: Links in Alphabetic Order
            robot1/base_footprint:
              Alpha: 1
              Show Axes: false
              Show Trail: false
            robot1/base_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot1/base_scan:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot1/caster_back_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot1/imu_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot1/wheel_left_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot1/wheel_right_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
          Name: Robot1
          Robot Description: robot1/robot_description
          TF Prefix: robot1
          Update Interval: 0
          Value: true
          Visual Enabled: true
        - Alpha: 1
          Class: rviz/RobotModel
          Collision Enabled: false
          Enabled: true
          Links:
            All Links Enabled: true
            Expand Joint Details: false
            Expand Link Details: false
            Expand Tree: false
            Link Tree Style: Links in Alphabetic Order
            robot2/base_footprint:
              Alpha: 1
              Show Axes: false
              Show Trail: false
            robot2/base_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot2/base_scan:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot2/caster_back_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot2/imu_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot2/wheel_left_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
            robot2/wheel_right_link:
              Alpha: 1
              Show Axes: false
              Show Trail: false
              Value: true
          Name: Robot2
          Robot Description: robot2/robot_description
          TF Prefix: robot2
          Update Interval: 0
          Value: true
          Visual Enabled: true
      Enabled: true
      Name: Robots
    - Class: rviz/TF
      Enabled: false
      Frame Timeout: 15
      Frames:
        All Enabled: true
      Marker Scale: 1
      Name: TF
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Tree:
        {}
      Update Interval: 0
      Value: false
  Enabled: true
  Global Options:
    Background Color: 48; 48; 48
    Default Light: true
    Fixed Frame: map
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz/Interact
      Hide Inactive: true
    - Class: rviz/MoveCamera
    - Class: rviz/Select
    - Class: rviz/FocusCamera
    - Class: rviz/Measure
    - Class: rviz/SetInitialPose
      Topic: /initialpose
    - Class: rviz/SetGoal
      Topic: /move_base_simple/goal
    - Class: rviz/PublishPoint
      Single click: true
      Topic: /clicked_point
  Value: true
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 20
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.06
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
        Value: false
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Focal Shape Fixed Size: true
      Focal Shape Size: 0.05
      Invert Z Axis: false
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 1.5708
      Target Frame: <Fixed Frame>
      Value: Orbit (rviz)
      Yaw: 0
    Saved: ~
